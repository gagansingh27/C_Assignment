#include <stdio.h>

int main()
{
    char chr;
    printf("Enter any character: ");
    scanf("%c", &chr);


    if(chr >= 'A' && chr <= 'Z')
    {
        printf("'%c' is uppercase alphabet.", chr);
    }
    else if(chr >= 'a' && chr <= 'z')
    {
        printf("'%c' is lowercase alphabet.", chr);
    }
    else
    {
        printf("'%c' is not an alphabet.", chr);
    }

    return 0;
}
