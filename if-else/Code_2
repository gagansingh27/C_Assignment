#include <stdio.h>

int main()
{
    int year;
    printf("Enter the year : ");
    scanf("%d", &year);

    if((year % 4 == 0) && (year % 100 !=0))
    {
        printf("Leap Year");
    }
    elif((year % 400==0))
    {
        printf("Leap Year")
    }
    else
    {
        printf("Common Year");
    }

    return 0;
}
